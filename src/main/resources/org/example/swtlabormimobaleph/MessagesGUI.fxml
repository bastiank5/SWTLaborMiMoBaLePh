          <TableColumn fx:id="employeeColumn" prefWidth="75.0" text="Employee" />
          <TableColumn fx:id="topicColumn" prefWidth="395.0" text="Topic" />
            <TableColumn fx:id="counterColumn" prefWidth="62.0" text="counter" />
