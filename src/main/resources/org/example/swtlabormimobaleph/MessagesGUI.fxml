      <Button fx:id="showEmployeeCalenderButton" layoutX="30.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleshowEmployeeCalender" text="Show Employee Calender" />
      <TextField fx:id="showEmployeeCalenderIDField" layoutX="117.0" layoutY="308.0" prefHeight="25.0" prefWidth="64.0" />
      <Label layoutX="30.0" layoutY="312.0" prefHeight="17.0" prefWidth="64.0" text="ID:" />
      <TextField fx:id="messageCounterField" layoutX="520.0" layoutY="308.0" prefHeight="25.0" prefWidth="44.0" />
